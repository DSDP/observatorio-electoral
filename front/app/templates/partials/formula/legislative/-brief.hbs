<div class="legislative-item -brief">
	<div class="item-title">
		<h5>{{formula.politicalForce.name}}</h5>
		<p>{{#if election.isDiputies}}
		Diputados
	{{else}}
		{{#if election.isSenators}}
			Senadores
		{{else}}
			Asambleistas
		{{/if}}
	{{/if}}</p>
	</div>

	<div class="item-content">	
	<div class="item">
		&nbsp;
		{{format-number formula.assemblyman.votes format="0.0"}}
	</div>
	<div class="item">
		&nbsp;
		{{formula.assemblyman.percent}}%
	</div>
	</div> 
</div> 	