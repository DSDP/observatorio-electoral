 <div class="field-container">
  <fieldset>
    <label>
      Propuesta
      {{input type="text" value=formula.proposal}}
    </label>
  </fieldset>


  <fieldset class="election-poll">
    <label>
       Votos SI
    </label>
    <div class="quantity">
      {{input type="number" value=formula.positiveVotes.votes placeholder="Ingrese la cantidad"}}
    </div>
    <div class="percent">
     ({{percent-of formula.positiveVotes.votes model.valids}})
    </div>
  </fieldset>   

  <fieldset class="election-poll">
    <label>
       Votos No
    </label>
    <div class="quantity">
      {{input type="number" value=formula.negativeVotes.votes placeholder="Ingrese la cantidad"}}
    </div>
    <div class="percent">
      ({{percent-of formula.negativeVotes.votes model.valids}})
    </div>
  </fieldset>   

</div>